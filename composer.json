{
    "name": "ackama/silverstripe-template-project",
    "type": "silverstripe-recipe",
    "description": "Ackama Silverstripe/CWP Template",
    "homepage": "https://ackama.com",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/totp-authenticator": "^4.0",
        "silverstripe/html5": "^2.0",
        "silverstripe/recipe-plugin": "^1.3",
        "silverstripe/recipe-blog": "^1.4",
        "cwp/agency-extensions": "^2.3",
        "cwp/cwp-recipe-core": "^2.4",
        "cwp/cwp-recipe-cms": "^2.4",
        "cwp/cwp-recipe-search": "^2.4",
        "cwp/cwp-pdfexport": "^1.1",
        "cwp/watea-theme": "^3.0",
        "dynamic/silverstripe-elemental-blocks": "^3.0",
        "dnadesign/silverstripe-elemental": "^4.2",
        "dnadesign/silverstripe-elemental-userforms": "^3.0",
        "phptek/sentry": "^3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "SilverstripeTemplateProject\\": [
                "app/src/"
            ],
            "SilverstripeTemplateProject\\Tests\\": [
                "app/tests/"
            ]
        },
        "classmap": [
            "app/helper/"
        ]
    },
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/blog.yml",
            "app/_config/environmentcheck.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/_config/search.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install.php",
            "public/.htaccess",
            "public/index.php",
            "public/web.config",
            "web.config"
        ],
        "expose": [
            "themes/silverstripe-template-theme/dist/css",
            "themes/silverstripe-template-theme/dist/fonts",
            "themes/silverstripe-template-theme/dist/ico",
            "themes/silverstripe-template-theme/dist/img",
            "themes/silverstripe-template-theme/dist/js"
        ]
    },
    "config": {
        "process-timeout": 600,
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
